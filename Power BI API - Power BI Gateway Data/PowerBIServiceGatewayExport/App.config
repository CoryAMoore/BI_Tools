<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

  <connectionStrings>
    <add name="DestDB" connectionString="Data Source=localhost;Initial Catalog=BITools;Integrated Security=SSPI" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="AzureADAuthorityURL" value="https://login.windows.net/common/oauth2/authorize" />  <!-- use with 3.19.8 - Do not change -->
    <add key="GlobalServiceEndpoint" value="https://api.powerbi.com" />  <!-- Cannot be left blank -->
    <add key="ServiceConfigPath" value="/powerbi/globalservice/v201606/environments/discover?user=" />  <!-- Cannot be left blank -->
    <add key="ServiceBackendPath" value="/spglobalservice/GetOrInsertClusterUrisByTenantLocation" />  <!-- Cannot be left blank -->

    <add key="OutputTableName" value="tmp.PBIGatewayAgent" />  <!-- Location to save data to (create from table create script) -->
    <add key="UserName" value="user@contoso.com" />  <!-- AD / PBI UserName, set to match your username -->
    <add key="Password" value="" />  <!-- AD / PBI UserName -->
  </appSettings>  
</configuration>